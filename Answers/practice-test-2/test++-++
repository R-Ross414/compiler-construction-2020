#include <stdio.h>

int main (){
    int x = 1; 
    printf("The result is: %d\n ", x++-++x) ; // -2
    printf("The result is: %d\n ", x++-x--) ; // -1
    printf("The result is: %d\n ", ++x-x--) ; // -0
    printf("The result is: %d\n ", ++x-(--x)) ; // -1
    printf("The result is: %d\n ", x+++(++x)) ; // -2
    printf("The result is: %d\n ", ++x*x+++(++x)) ; // -2
    printf("The result is: %d\n ", --x-x+(++x)) ; // 
    }